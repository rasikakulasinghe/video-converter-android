{
  "name": "video-converter-android",
  "version": "1.0.0",
  "description": "Mobile Video Converter Android App - Offline video conversion to web-optimized MP4 format",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "build:android": "cd android && gradlew.bat assembleRelease",
    "build:android:debug": "cd android && gradlew.bat assembleDebug",
    "build:android:release": "cd android && gradlew.bat assembleRelease",
    "build:android:bundle": "cd android && gradlew.bat bundleRelease",
    "build:android:clean": "cd android && gradlew.bat clean",
    "analyze:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android-bundle.js --assets-dest ./",
    "clean": "cd android && gradlew.bat clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "clsx": "^2.1.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.73.9",
    "react-native-device-info": "^10.0.0",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^15.13.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.0",
    "@react-native/metro-config": "^0.73.0",
    "@react-native/typescript-config": "^0.73.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.2",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "react-native",
    "android",
    "video-converter",
    "ffmpeg",
    "mobile",
    "offline"
  ],
  "author": "Video Converter Team",
  "license": "MIT"
}
