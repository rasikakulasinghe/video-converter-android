{
  "name": "video-converter-expo",
  "version": "1.0.0",
  "description": "Mobile Video Converter Expo App - Offline video conversion to web-optimized MP4 format",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:development": "eas build --platform android --profile development",
    "prebuild": "expo prebuild",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/checkbox": "^0.5.20",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.2.0",
    "clsx": "^2.1.1",
    "expo": "~52.0.47",
    "expo-asset": "~11.0.5",
    "expo-av": "~15.0.2",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.19",
    "expo-device": "~7.0.3",
    "expo-document-picker": "~13.0.3",
    "expo-file-system": "~18.0.12",
    "expo-font": "~13.0.4",
    "expo-image-picker": "~16.0.6",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "4.0.9",
    "nativewind": "^2.0.11",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-device-info": "^14.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.3.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "jest": "^29.6.3",
    "jest-expo": "~52.0.4",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "18.3.1",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.2",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-fs",
          "react-native-haptic-feedback",
          "nativewind",
          "clsx",
          "zustand",
          "@react-native-community/checkbox"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "expo",
    "react-native",
    "android",
    "video-converter",
    "ffmpeg",
    "mobile",
    "offline"
  ],
  "author": "Video Converter Team",
  "license": "MIT",
  "private": true
}
